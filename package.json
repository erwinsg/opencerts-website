{
  "name": "with-redux",
  "version": "1.0.1",
  "scripts": {
    "dev": "next",
    "lint": "eslint . --ext .js --max-warnings 0",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next build && next start",
    "cname": "echo 'opencerts.io' > out/CNAME",
    "copy-static-files": "cp docs/exampleIssuers.json out/exampleIssuers.json",
    "deploy": "yarn deploy:prebuilt && gh-pages --dotfiles --dist out",
    "deploy:prebuilt": "rm -rf out && yarn build:static && touch out/.nojekyll && yarn cname && yarn copy-static-files"
  },
  "dependencies": {
    "@govtechsg/open-certificate": "^1.1.33",
    "@zeit/next-sass": "^0.2.0",
    "eslint-plugin-react": "^7.7.0",
    "ethereum-ens": "^0.7.4",
    "ethereumjs-util": "^5.1.5",
    "ledger-wallet-provider": "^2.0.1-beta.2",
    "lodash": "^4.17.5",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "truffle-contract": "^3.0.4",
    "web3": "^1.0.0-beta.33",
    "web3-provider-engine": "^13.6.5"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.10.2"
  }
}
